<form class="form-horizontal">
  <div class="form-group">
    <label for="name" class="col-sm-1 control-label">Name</label>
    <input name="name"
           id="name"
           class="col-sm-1"
           ng-model="gnc.data.name"
           type="text">
  </div>

  <div class="form-group">
    <label for="name" class="col-sm-1 control-label">Algorithm</label>
    <select ng-model="gnc.data.algorithm">
      <option value="">Select</option>
      <option value="contagion">Contagion</option>
    </select>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-1 col-sm-1">
      <button ng-click="gnc.save()"
              type"submit"
              class="btn btn-default">
        Save
      </button>
    </div>
  </div>
</form>

